{
  "cli": {
	"version": "2.6.0"
  },
  "build": {
	"production": {
	  "yarn": "1.22.19",
	  "distribution": "store",
	  "channel": "production",
	  "env": {
		"APP_ENV": "{{APP_ENV}}",
		"GOOGLE_SERVICES_JSON": "/"
	  }
	},
	"beta": {
	  "extends": "production",
	  "channel": "beta"
	},
	"alpha": {
	  "extends": "production",
	  "distribution": "internal",
	  "channel": "alpha",
	  "ios": {
		"simulator": true
	  },
	  "android": {
		"buildType": "apk"
	  }
	},
	"development": {
	  "extends": "production",
	  "channel": "development",
	  "developmentClient": true,
	  "distribution": "internal",
	  "ios": {
		"simulator": true
	  }
	}
  },
  "submit": {
	"production": {
	  "android": {
		"track": "production",
		"releaseStatus": "draft",
		"serviceAccountKeyPath": "{{DEV_GOOGLE_SERVICE_KEY_PATH}}"
	  },
	  "ios": {
		"ascApiKeyPath": "{{DEV_APPLE_API_KEY_PATH}}",
		"ascApiKeyIssuerId": "{{DEV_APPLE_API_KEY_ISSUER_ID}}",
		"ascApiKeyId": "{{DEV_APPLE_API_KEY_ID}}",
		"appleId": "{{DEV_APPLE_ID}}",
		"ascAppId": "{{DEV_APP_ID}}",
		"appleTeamId": "{{DEV_TEAM_ID}}"
	  }
	},
	"beta": {
	  "extends": "release",
	  "android": {
		"track": "beta"
	  }
	},
	"internal": {
	  "extends": "release",
	  "android": {
		"track": "internal",
		"releaseStatus": "completed"
	  }
	}
  }
}